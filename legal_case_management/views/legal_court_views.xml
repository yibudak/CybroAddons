<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Tree view For Court -->
    <record id="legal_court_view_tree" model="ir.ui.view">
        <field name="name">legal.court.tree</field>
        <field name="model">legal.court</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
                <field name="judge_id"/>
            </tree>
        </field>
    </record>
</odoo>
