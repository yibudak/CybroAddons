<?xml version="1.0" encoding="UTF-8" ?>
    <templates id="template" xml:space="preserve">
      <t t-name="theme_eco_food.eco_food_featured_products1" name="Eco Food Featured Products">
        <section class="featured_product">
            <div class="f_wrapper">
                <div class="container-fluid">
                    <div class="row justify-content-lg-end">
                        <div class="col-lg-6 col-12">
                            <div class="featured_wrapper">
                                <div class="main_heading">
                                    <h1>
                                        featured product
                                    </h1>
                                    <p>
                                        Add featured products to weekly line up
                                    </p>
                                </div>
                                <div class="featured_slider">
                                    <div class="wrapper">
                                        <div class="owl-carousel featured_eco_food favorite-carousel   owl-loaded owl-drag" id="featured_eco_food">
                                            <div>
                                                <div class="row">
                                                    <t t-foreach="slide1" t-as="fp"
                                                       t-if="slide1">
                                                        <div class="col-lg-6 col-md-6 col-sm-12 col-12">
                                                            <div class="b_product">
                                                                <a t-attf-href="/shop/product/#{(fp[0].id)}">
                                                                    <div class="wrapper">
                                                                        <div class="img_wrapper">
                                                                         <img t-attf-src="/web/image?model=product.template&amp;field=image_1920&amp;id=#{fp[0].id}"/>
                                                                            <div class="inner">
                                                                                <a t-attf-href="/shop/product/#{(fp[0].id)}"
                                                                                   itemprop="url" type="button"
                                                                                   class="quick_view scale-in-center">
                                                                                    <i class="fa fa-search"/>
                                                                                </a>
                                                                            </div>
                                                                        </div>
                                                                        <div class="product_info">
                                                                            <div class="p_caregory">
                                                                                <span t-if="slide1"
                                                                                      t-esc="fp[0].categ_id.name"/>
                                                                            </div>
                                                                            <div class="p_name">
                                                                                <span t-if="slide1"
                                                                                      t-esc="fp[0].name"/>
                                                                            </div>
                                                                            <div class="p_rating ">

                                                <div class="p_rating ">
                                                    <div class="o_website_rating_static"
                                                         style="display:flex;"
                                                         t-att-style="inline_mode and 'display:inline'"
                                                         t-att-title="rating_avg">
                                                        <t t-call="portal_rating.rating_stars_static">
                                                            <t t-set="val" t-value="fp[0].rating_avg || 0"/>
                                                        </t>
                                                    </div>
                                                </div>
                                                                            </div>

                                                                            <div class="p_price ">
                                                                                <span t-if="slide1"
                                                                                      t-esc="fp[0].list_price"/>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </t>
                                                </div>
