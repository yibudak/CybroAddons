<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="fasion_snippets" inherit_id="website.snippets" name="Snippets">
        <xpath expr="//div[@id='snippet_feature']/div[hasclass('o_panel_body')]"
               position="inside">
            <t t-snippet="theme_fasion.s_fasion_about"
               t-thumbnail="/theme_fasion/static/src/images/snippets/about.jpg"/>
            <t t-snippet="theme_fasion.s_fasion_banner"
               t-thumbnail="/theme_fasion/static/src/images/snippets/index-banner-block.jpg"/>
            <t t-snippet="theme_fasion.s_fasion_cart"
               t-thumbnail="/theme_fasion/static/src/images/snippets/cart-block.jpg"/>
            <t t-snippet="theme_fasion.s_fasion_contact"
               t-thumbnail="/theme_fasion/static/src/images/snippets/contact-block.jpg"/>
            <t t-snippet="theme_fasion.s_fasion_preview"
               t-thumbnail="/theme_fasion/static/src/images/snippets/product-preview-block.jpg"/>
            <t t-snippet="theme_fasion.s_fasion_product_blog"
               t-thumbnail="/theme_fasion/static/src/images/snippets/blogs-block.jpg"/>
            <t t-snippet="theme_fasion.s_fasion_product"
               t-thumbnail="/theme_fasion/static/src/images/snippets/product-fasion-block.jpg"/>
            <t t-snippet="theme_fasion.s_fasion_slider"
               t-thumbnail="/theme_fasion/static/src/images/snippets/fasion-banner-block.jpg"/>
             <t t-snippet="theme_fasion.s_fasion_sliders"
               t-thumbnail="/theme_fasion/static/src/images/snippets/slider-block.jpg"/>
        </xpath>
    </template>
</odoo>