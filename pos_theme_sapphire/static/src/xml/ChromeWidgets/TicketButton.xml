<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <!-- Orders details button -->
    <t t-name="TicketButton" t-inherit="point_of_sale.TicketButton" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('ticket-button')]" position="replace">
            <div class="ticket-button menu-txt-hld with-badge" t-on-click="onClick" t-att-badge="count">
                <i class="fa fa-ticket menu-icon"/>
                <span class="menu-txt">Orders</span>
            </div>
        </xpath>
    </t>
</templates>
